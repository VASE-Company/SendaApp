{
  "name": "senda-app",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:mobile": "npm run dev --workspace @senda/mobile",
    "dev:web": "npm run dev --workspace @senda/web",
